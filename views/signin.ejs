<%- include('partials/header'); -%>
<div class="container">
<div class="text-center sign">
    <form class="form-signin" action="/login" method="post">
      <h1 class="h3 mb-3 font-weight-normal">Admin</h1>
      <label for="inputEmail" class="sr-only">Username</label>
      <input type="text" id="inputUser" class="form-control" placeholder="Username" name="userName" required autofocus>
      <label for="inputPassword" class="sr-only">Password</label>
      <input type="password" id="inputPassword" class="form-control" name="userPswd" placeholder="Password" required>
      <button class="btn btn-lg btn-primary btn-block" type="submit">Go!</button>
    </form>
  </div>
<%- include('partials/footer'); -%>
